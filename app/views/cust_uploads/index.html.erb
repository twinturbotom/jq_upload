<h1>Upload</h1>
<div id="cust_uploads">
  <%= render @cust_uploads %>
</div>
<div class="clear"></div>

<%= s3_uploader_form post: cust_uploads_url, as: "cust_upload[cust_file_url]" do %>
  <%= file_field_tag :file, multiple: true %>
<% end %>

<script id="template-upload" type="text/x-tmpl">
<div class="upload">
  {%=o.name%}
  <div class="progress"><div class="bar" style="width: 0%"></div></div>
</div>
</script>
